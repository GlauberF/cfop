{
    "name": "jansenfelipe/cfop",
    "description": "Lista atualizada de CFOP",
    "keywords": ["erp", "openerp", "erp php", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "laravel/framework": "5.1.*",
        "artesaos/defender": "0.2.16",
        "laraerp/ordination": "^1.0",
        "jansenfelipe/utils": "^2.0",
        "jansenfelipe/cnpj-gratis": "^2.0",
        "jansenfelipe/cpf-gratis": "^2.0",
        "jansenfelipe/cep-gratis": "^2.0",
        "jansenfelipe/nfephp-serialize": "^3.10"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "^0.9.4",
        "heroku/heroku-buildpack-php": "^71.0"
    },
    "autoload": {
        "classmap": [
            "laraerp/database"
        ],
        "psr-4": {
            "App\\": "laraerp/app",
            "Laraerp\\Core\\": "core/src",
            "Laraerp\\Pessoa\\": "pessoa/src",
            "Laraerp\\Contato\\": "contato/src",
            "Laraerp\\Endereco\\": "endereco/src",
            "Laraerp\\Correios\\": "correios/src",
            "Laraerp\\Cidade\\": "cidade/src",
            "Laraerp\\Template\\": "template/src"
        }
    },
    "scripts": {
      "post-install-cmd": [
        "php -r \"copy('laraerp/.env.example', 'laraerp/.env');\"",
        "php artisan key:generate",
        "php artisan clear-compiled"
      ],
      "post-update-cmd": [
        "php artisan clear-compiled"
      ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
